<resources>
    <!-- Primary color of the app (shown in the app bar) -->
    <color name="colorPrimary">#2B3D4D</color>

    <!-- Primary dark color of the app (shown in the status bar) -->
    <color name="colorPrimaryDark">#23303D</color>

    <!-- Accent color of the app -->
    <color name="colorAccent">#10CAC9</color>

    <!-- Text color for the author of the book in the list item -->
    <color name="textColorAuthor">#B4BAC0</color>

    <!-- Text color for the title of the book in the list item -->
    <color name="textColorTitle">#2B3D4D</color>

    <!-- Color for a book with book number 0 and 2 -->
    <color name="color1">#4A7BA7</color>

    <!-- Magnitude circle color for a book with book number between 2 and 3 -->
    <color name="color2">#df9c86</color>

    <!-- Magnitude circle color for a book with book number between 3 and 4 -->
    <color name="color3">#a9cfed</color>

    <!-- Magnitude circle color for a book with book number between 4 and 5 -->
    <color name="color4">#F5A623</color>

    <!-- Magnitude circle color for a book with book number between 5 and 6 -->
    <color name="color5">#FF7D50</color>

    <!-- Magnitude circle color for a book with book number between 6 and 7 -->
    <color name="color6">#01a979</color>

    <!-- Magnitude circle color for a book with book number between 7 and 8 -->
    <color name="color7">#0c24c4</color>

    <!-- Magnitude circle color for a book with book number between 8 and 9 -->
    <color name="color8">#edf506</color>

    <!-- Magnitude circle color for a book with book number between 9 and 10 -->
    <color name="color9">#801504</color>

    <!-- Magnitude circle color for a book with book number over 10 -->
    <color name="color10">#C03823</color>
</resources>
